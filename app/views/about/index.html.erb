<body>

<!-- PRELOADER -->
<div class="preloader">
    <div class="inner">
        <div class="item item1"></div>
        <div class="item item2"></div>
        <div class="item item3"></div>
    </div>
</div>
<!-- END / PRELOADER -->

<!-- PAGE WRAP -->
<div id="page-wrap">
    
    <!-- HEADER -->
        <%= render "home/header" %>
    <!-- END / HEADER -->

    <!-- SUB BANNER -->
    <section class="sub-banner text-center section">
        <div class="awe-parallax bg-5"></div>
        <div class="awe-title awe-title-3">
            <h3 class="lg text-uppercase">About</h3>
        </div>
    </section>
    <!-- END / SUB BANNER -->

    <!-- ABOUT STORY -->
    <section id="about-story" class="about-story section">
        <div class="divider divider-2"></div>
        <div class="container">
            <div class="block-first">
                <div class="row">
                    <div class="col-md-4">
                        <div class="image-wrap">
                            <img src="images/logo-main.jpg" alt="">
                        </div>
                    </div>

                    <div class="col-md-7 col-md-offset-1">
                        <h4 class="lg text-uppercase">HEALTHY FOOD IS FUTURE</h4>
                        <p>Venison spare ribs pork tail, beef meatloaf kielbasa turkey rump turducken beef ribs ground round sirloin kevin hamburger. Boudin ham hock chicken,. Leberkas ham sausage doner pastrami porchetta tri-tip boudin shankle. Meatball shoulder rump spare ribs tongue venison short ribs. Flank landjaeger brisket leberkas bacon kielbasa shank meatball, fatback t-bone pork biltong boudin. Hamburger strip steak ribeye pastrami tenderloin.</p>
                    </div>
                </div>
            </div>

            <div class="block-last">
                <div class="row">
                    <div class="col-md-4 col-md-push-8">
                        <div class="row">
                            <div class="col-xs-6">
                                <div class="image-wrap">
                                    <img src="images/about/img-2.jpg" alt="">
                                </div>
                            </div>

                            <div class="col-xs-6">
                                <div class="image-wrap">
                                    <img src="images/about/img-3.jpg" alt="">
                                </div>
                            </div>

                            <div class="col-xs-6">
                                <div class="image-wrap">
                                    <img src="images/about/img-4.jpg" alt="">
                                </div>
                            </div>

                            <div class="col-xs-6">
                                <div class="image-wrap">
                                    <img src="images/about/img-5.jpg" alt="">
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-7 col-md-pull-4">
                        <h4 class="lg text-uppercase">WE ONLY MAKE AMBROSIAL MEAL</h4>
                        <p>Venison spare ribs pork tail, beef meatloaf kielbasa turkey rump turducken beef ribs ground round sirloin kevin hamburger. Boudin ham hock chicken,. Leberkas ham sausage doner pastrami porchetta tri-tip boudin shankle. Meatball shoulder rump spare ribs tongue venison short ribs. Flank landjaeger brisket leberkas bacon kielbasa.</p>
                        <a href="menu.html" class="awe-btn awe-btn-2 awe-btn-default text-uppercase">See menu</a>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ABOUT STORY -->

    <!-- FOOTER -->
        <footer id="footer" class="footer">
            <div class="awe-color"></div>
            <div class="container">
                <div class="copyright text-center">
                    <%= render "home/footer" %>
                </div>
            </div>
        </footer>
    <!-- END / FOOTER -->
    

</div>
<!-- END / PAGE WRAP -->


<!-- LOAD JQUERY -->
<%= javascript_include_tag '/frontend/js/lib/jquery-1.11.2.min.js' %>

<!-- GOOGLE MAP -->
<%= javascript_include_tag 'http://maps.google.com/maps/api/js?sensor=false' %>
<%= javascript_include_tag '/frontend/js/lib/bootstrap.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.bxslider.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.easing.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.owl.carousel.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/masonry.pkgd.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/perfect-scrollbar.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.magnific-popup.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.parallax-1.1.3.js' %>
<%= javascript_include_tag '/frontend/js/lib/retina.min.js' %>
<%= javascript_include_tag '/frontend/js/scripts.js' %>

</body>