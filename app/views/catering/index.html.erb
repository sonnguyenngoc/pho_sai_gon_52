<body>

<!-- PRELOADER -->
<div class="preloader">
    <div class="inner">
        <div class="item item1"></div>
        <div class="item item2"></div>
        <div class="item item3"></div>
    </div>
</div>
<!-- END / PRELOADER -->

<!-- PAGE WRAP -->
<div id="page-wrap">
    
    <!-- HEADER -->
        <%= render "home/header" %>
    <!-- END / HEADER -->

    <!-- EVENTS -->
    <section id="events" class="events section">
        <div class="divider divider-2"></div>
        <div class="section-content">
            <div class="container">
                <div class="item-event">
                    <div class="row">
                        <div class="col-sm-4">
                            <div class="image-wrap">
                                <img src="/frontend/images/temp/15.jpg" alt=""> 
                            </div>
                        </div>
                        <div class="col-sm-8">
                            <div class="item-event-body">
                                <div class="item-event-title">
                                    <h3>Phở Sài Gòn 52</h3>
                                    <h5>Take Away - Asia Buffet - Catering - Lieferung</h5>
                                </div>
                                <div class="item-event-content">
                                    <p>Address: Zürcherstrasse 91, 8952 Schlieren, Switzerland</p>
                                    <p>Telefon: 043 495 00 22 - Fax: 043 495 00 22</p>
                                    <p>Email: phosaigon52@gmail.com</p>
                                    <p>Website: <%= link_to "phosaigon52.ch", "http://phosaigon52.ch/", target: "_blank" %></p>
                                    <p>See location: <%= link_to "here", "https://www.google.com/maps/place/Z%C3%BCrcherstrasse+91,+8952+Schlieren,+Thu%E1%BB%B5+S%C4%A9/@47.3981939,8.4569929,17z/data=!3m1!4b1!4m2!3m1!1s0x47900b8eb1097ab9:0x190058072b37d7a4", target: "_blank" %></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- END / EVENTS -->

    <!-- FOOTER -->
    <footer id="footer" class="footer">
        <div class="awe-color"></div>
        <div class="container">
            <div class="copyright text-center">
                <%= render "home/footer" %>
            </div>
        </div>
    </footer>
    <!-- END / FOOTER -->
    

</div>
<!-- END / PAGE WRAP -->

<!-- LOAD JQUERY -->
<%= javascript_include_tag '/frontend/js/lib/jquery-1.11.2.min.js' %>

<!-- GOOGLE MAP -->
<%= javascript_include_tag 'http://maps.google.com/maps/api/js?sensor=false' %>
<%= javascript_include_tag '/frontend/js/lib/bootstrap.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.bxslider.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.easing.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.owl.carousel.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/masonry.pkgd.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/perfect-scrollbar.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.magnific-popup.min.js' %>
<%= javascript_include_tag '/frontend/js/lib/jquery.parallax-1.1.3.js' %>
<%= javascript_include_tag '/frontend/js/lib/retina.min.js' %>
<%= javascript_include_tag '/frontend/js/scripts.js' %>

</body>
</html>